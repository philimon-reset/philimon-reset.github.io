---
import BaseLayout from "../layouts/BaseLayout.astro";
import HorizontalCard from "../components/HorizontalCard.astro";
const tags = ["rust"];
---

<BaseLayout title="Projects" sideBarActiveItemID="projects">
  <div>
    <div class="text-3xl w-full font-bold mb-5">My Projects</div>
  </div>

  <HorizontalCard
    title="SoftRenderer"
    img="/demo.gif"
    desc="This project is a 3D rendering engine implemented in pure C#, which supports both rasterization and ray tracing rendering techniques."
    url="https://github.com/philimon-reset/SoftRenderer"
    badge="IN PROGRESS"
  />
  <div class="divider my-0"></div>
  <HorizontalCard
    title="Conway's Game Of Life"
    img="/giphy.webp"
    desc="This project is an implementation of Conway's Game of Life built with a custom C# renderer."
    url="https://github.com/philimon-reset/Conways-Game-Of-Life"
  />
  <div class="divider my-0"></div>
  <HorizontalCard
    title="GrubOn"
    img="/grubon.webp"
    desc="Grub On tackles food waste and fosters community connections by allowing individuals and businesses to share or sell surplus food."
    url="https://github.com/philimon-reset/grubOn"
    badge="GDSC Top 100 Finalist"
  />
  <div class="divider my-0"></div>

  <HorizontalCard
    title="Simple Shell."
    img="/shell.webp"
    desc="This project involves the development of a basic UNIX command-line interpreter, often referred to as a shell. The command interpreter allows users to interact with the operating system by typing commands, which the shell interprets and executes."
    url="https://github.com/philimon-reset/simple_shell"
  />
  <div class="divider my-0"></div>
  <HorizontalCard
    title="GeoChat"
    img="/geochat.webp"
    desc="This project is a real-time web-based chatting application built using React for the front end, Redis for managing real-time data and message queues, and MongoDB for storing user data and chat histories."
    url="https://github.com/philimon-reset/GeoChat.io"
  />
  <div class="divider my-0"></div>
  <HorizontalCard
    title="Airbnb Backend clone"
    img="/airbnb.png"
    desc="This project involves building the backend system for an Airbnb-style platform that enables users to list, search, and book accommodations. The backend is responsible for managing user authentication, property listings, search functionality, booking processes and more."
    url="https://github.com/philimon-reset/AirBnB_clone_v3"
  />
</BaseLayout>
